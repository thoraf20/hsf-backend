{
  "name": "new-hrf-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .ts src/",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "test": "jest --config jest.config.js",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "migrate:dev": "knex migrate:latest --env development --knexfile=database/knexfile.ts",
    "migrate:prod": "knex migrate:latest --env production --knexfile=database/knexfile.ts",
    "make": "knex migrate:make attendance_escrow -x ts --knexfile=database/knexfile.ts",
    "make:seed": "knex seed:make roles_and_permissions -x ts --knexfile=database/knexfile.ts",
    "seed:run": "npx knex seed:run --knexfile=database/knexfile.ts",
    "seed:developer": "npx knex seed:run --specific=developer_user.ts --knexfile=database/knexfile.ts",
    "seed:review_request": "npx knex seed:run  --knexfile=database/knexfile.ts --specific=review_requests.ts",
    "seed:hsf_org": "npx knex seed:run --knexfile=database/knexfile.ts --specific=hsf_organization.ts",
    "seed:notification_data": "npx knex --knexfile database/knexfile.ts seed:run --specific=initial_notification_data.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@oslojs/binary": "^1.0.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@oslojs/otp": "^1.1.0",
    "@sendgrid/mail": "^8.1.4",
    "@types/hpp": "^0.2.6",
    "@types/multer": "^1.4.12",
    "@types/qrcode": "^1.5.5",
    "arctic": "^3.6.0",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.51.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-brute": "^1.0.1",
    "express-rate-limit": "^7.5.0",
    "express-slow-down": "^2.0.3",
    "express-validator": "^7.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "luxon": "^3.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "new-hrf-api": "file:",
    "pg": "^8.14.1",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.2.0",
    "ulid": "^3.0.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.3",
    "@swc/core": "^1.11.21",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.13",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
